/ {
	aliases {
		spi0 = "";
		spi7 = "";
		spi1 = &blsp1_spi;
		spi2 = &blsp2_spi;
		serial1 = &blsp6_serial;
/*		serial3 = &blsp8_serial;*/
	};
};

&soc {

	/*---------------------*/
	/* BLSP1 QUP0(BLSP#1)  */
	/*---------------------*/
	blsp1_spi: spi@f9923000 {
		status = "ok";
		spi-max-frequency = <50000000>;
		qcom,gpio-cs0  = <&msmgpio 2 0>;
		qcom,use-bam;
	};

	/*---------------------*/
	/* BLSP1 QUP1(BLSP#2)  */
	/*---------------------*/
	blsp2_spi: spi@f9924000 {
		status = "ok";
		qcom,gpio-cs0  = <&msmgpio 6 0>;
		shtps_rmi@0 {
			status = "ok";
			compatible = "sharp,shtps_rmi";
			reg = <0>;
			spi-max-frequency = <400000>;
			interrupt-parent = <&msmgpio>;
			interrupts = <61 0>;
			spi-cpol;
			spi-cpha;

			/* user defined property */
			shtps_rmi,rst_pin = <&pm8941_gpios 30 0>;
			shtps_rmi,vdd_3_0V = <&pm8941_l15>;
			shtps_rmi,vbus_1_8V = <&pm8941_lvs1>;
		};
	};

	/*---------------------*/
	/* BLSP1 QUP2(BLSP#3)  */
	/*---------------------*/

	/*---------------------*/
	/* BLSP1 QUP3(BLSP#4)  */
	/*---------------------*/

	/*---------------------*/
	/* BLSP1 QUP4(BLSP#5)  */
	/*---------------------*/
	blsp5_i2c: i2c@f9927000 {
		status = "ok";
	};

	/*---------------------*/
	/* BLSP1 QUP5(BLSP#6)  */
	/*---------------------*/
	blsp6_serial: serial@f9922000 {
		status = "ok";
	};

	/*---------------------*/
	/* BLSP2 QUP0(BLSP#7)  */
	/*---------------------*/

	/*---------------------*/
	/* BLSP2 QUP1(BLSP#8)  */
	/*---------------------*/
	blsp8_serial: serial@f995e000 {
		sharp,use-irda;
		qcom,config-gpio = <0>;
		status = "ok";
	};

	/*---------------------*/
	/* BLSP2 QUP2(BLSP#9)  */
	/*---------------------*/

	/*---------------------*/
	/* BLSP2 QUP3(BLSP#10) */
	/*---------------------*/

	/*---------------------*/
	/* BLSP2 QUP4(BLSP#11) */
	/*---------------------*/

	/*---------------------*/
	/* BLSP2 QUP5(BLSP#12) */
	/*---------------------*/
	blsp12_i2c: i2c@f9968000 {
		status = "ok";
	};

