		qcom,gpio-clk  = <&msmgpio 7 0>;
/*		qcom,gpio-cs0  = <&msmgpio 6 0>;*/
		qcom,gpio-cs0  = <>;

		qcom,infinite-mode = <0>;
/*		qcom,use-bam;*/
		qcom,ver-reg-exists;
		qcom,bam-consumer-pipe-index = <14>;
		qcom,bam-producer-pipe-index = <15>;
		qcom,master-id = <86>;

		status = "disabled";
